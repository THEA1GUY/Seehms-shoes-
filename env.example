# Turso (Database)
# Get these from: turso db show --url <db-name> and turso db tokens create <db-name>
TURSO_DATABASE_URL="libsql://your-db-name-org.turso.io"
TURSO_AUTH_TOKEN="your-auth-token"

# Cloudinary (Images)
# Get these from your Cloudinary Dashboard
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME="your-cloud-name"
CLOUDINARY_API_KEY="your-api-key"
CLOUDINARY_API_SECRET="your-api-secret"

# NextAuth (Authentication)
# Generate a secret with: openssl rand -base64 32
AUTH_SECRET="your-super-secret-key"
# Base URL (http://localhost:3000 for dev)
AUTH_URL="http://localhost:3000"

# Admin User (Simple Credential)
ADMIN_EMAIL="admin@seemsshoes.com"
ADMIN_PASSWORD="secure-password-change-me"
